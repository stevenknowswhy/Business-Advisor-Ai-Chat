{
  "description": "AI Advisor Chat - Model-agnostic AI advisor platform using Open Router with multiple AI model support",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c jest.config.ts --runInBand",
    "test:ci": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c jest.config.ts --ci --runInBand",
    "test:marketplace": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c jest.config.ts --runInBand --testPathPatterns=\"marketplace|ui\"",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c jest.config.ts --runInBand --watch"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@ai-sdk/react": "^2.0.34",
    "@clerk/nextjs": "^6.32.0",
    "@convex-dev/auth": "^0.0.89",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/stripe": "^8.0.416",
    "@vercel/blob": "^2.0.0",
    "ai": "^5.0.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.27.1",
    "date-fns": "^4.1.0",
    "express-rate-limit": "^8.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.544.0",
    "next": "^15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.63.0",
    "server-only": "^0.0.1",
    "stripe": "^18.5.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
